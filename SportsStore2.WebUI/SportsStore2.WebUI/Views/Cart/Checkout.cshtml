@model SportsStore2.Domain.Entities.ShippingDetails

@{
    ViewBag.Title = "Checkout";
}

<h2>Checkout</h2>

@Html.ValidationSummary()

@if (ViewData.ModelState["emptyCart"] == null) {
    using (Html.BeginForm()) {
        @Html.EditorForModel()
        <input type="submit" value="Complete Checkout" />
    }
}